    "reactotron-react-native": "3.6.5",
    "reactotron-redux": "3.1.1"
  },
  "jest": {
    "preset": "react-native",
    "setupFilesAfterEnv": [
      "./setup-tests.js"
    ],
    "collectCoverage": false,
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "html"
    ]
  }
